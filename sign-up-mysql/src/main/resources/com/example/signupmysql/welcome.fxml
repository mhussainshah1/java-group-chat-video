<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18"
            fx:controller="com.example.signupmysql.WelcomeController">
    <children>
        <Label fx:id="label_welcome" layoutX="238.0" layoutY="76.0" text="Welcome!">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0"/>
            </font>
        </Label>
        <Label fx:id="label_fav_channel" layoutX="95.0" layoutY="176.0"
               text="Your Favourite Youtube Channel is WittCode!">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0"/>
            </font>
        </Label>
        <Button fx:id="button_logout" layoutX="223.0" layoutY="280.0" mnemonicParsing="false" onAction="#btnHandler"
                prefHeight="31.0" prefWidth="154.0" style="-fx-background-color: #FED700;" text="Logout">
            <font>
                <Font name="Arial Rounded MT Bold" size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
